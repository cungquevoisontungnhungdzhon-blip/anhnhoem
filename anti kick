-- ⚠️ Soft AntiKick: chỉ chặn Kick gọi trực tiếp từ local
local Players = game:GetService("Players")
local plr = Players.LocalPlayer

-- Thử thay thế method Kick
pcall(function()
    rawset(plr, "Kick", function(...)
        warn("🚫 Soft AntiKick: Kick attempt blocked")
        return nil
    end)
end)

-- KickLocalPlayer (nếu tồn tại)
pcall(function()
    rawset(plr, "KickLocalPlayer", function(...)
        warn("🚫 Soft AntiKick: KickLocalPlayer blocked")
        return nil
    end)
end)

print("✅ Soft AntiKick loaded")

