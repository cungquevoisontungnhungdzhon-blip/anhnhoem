-- âš ï¸ Soft AntiKick: chá»‰ cháº·n Kick gá»i trá»±c tiáº¿p tá»« local
local Players = game:GetService("Players")
local plr = Players.LocalPlayer

-- Thá»­ thay tháº¿ method Kick
pcall(function()
    rawset(plr, "Kick", function(...)
        warn("ğŸš« Soft AntiKick: Kick attempt blocked")
        return nil
    end)
end)

-- KickLocalPlayer (náº¿u tá»“n táº¡i)
pcall(function()
    rawset(plr, "KickLocalPlayer", function(...)
        warn("ğŸš« Soft AntiKick: KickLocalPlayer blocked")
        return nil
    end)
end)

print("âœ… Soft AntiKick loaded")

